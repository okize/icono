{
  "name": "iconr",
  "version": "0.8.1",
  "description": "CLI tool that consumes a directory of SVG images and outputs a CSS file of inlined data as well as a directory of PNG images for fallback if necessary.",
  "preferGlobal": true,
  "main": "./bin/iconr.js",
  "bin": {
    "iconr": "./bin/iconr.js"
  },
  "keywords": [
    "svg",
    "png",
    "css",
    "datauri",
    "fallback"
  ],
  "scripts": {
    "test": "./node_modules/.bin/gulp test",
    "clean": "./node_modules/.bin/gulp clean",
    "build": "./node_modules/.bin/gulp build",
    "prepublish": "npm run clean && npm run build"
  },
  "dependencies": {
    "bluebird": "^2.9.25",
    "chalk": "^1.0.0",
    "cssbeautify": "^0.3.0",
    "gzip-size": "^1.0.0",
    "lodash": "^3.0.0",
    "microtime": "^1.2.0",
    "mime": "^1.2.0",
    "mkdirp": "^0.5.0",
    "phantomjs": "^1.9.0",
    "progger": "^1.0.0",
    "rimraf": "^2.2.0",
    "svgo": "^0.5.0",
    "yargs": "^3.10.0"
  },
  "devDependencies": {
    "babel-eslint": "*",
    "chai": "*",
    "coffee-script": "*",
    "coveralls": "*",
    "del": "*",
    "gulp": "*",
    "gulp-babel": "*",
    "gulp-coveralls": "*",
    "gulp-eslint": "*",
    "gulp-help": "*",
    "gulp-mocha": "*",
    "gulp-plumber": "*",
    "gulp-rename": "*",
    "gulp-template": "*",
    "gulp-util": "*",
    "json2markdown": "*",
    "mocha": "*",
    "mocha-lcov-reporter": "*",
    "run-sequence": "*"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okize/iconr.git"
  },
  "bugs": "https://github.com/okize/iconr/issues",
  "homepage": "https://github.com/okize/iconr/",
  "author": {
    "name": "Morgan Wigmanich",
    "email": "okize123@gmail.com",
    "url": "https://github.com/okize"
  },
  "license": "MIT"
}
