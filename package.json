{
  "name": "iconr",
  "version": "0.8.5",
  "description": "CLI tool that consumes a directory of SVG images and outputs a CSS file of inlined data as well as a directory of PNG images for fallback if necessary.",
  "preferGlobal": true,
  "main": "./bin/iconr.js",
  "bin": {
    "iconr": "./bin/iconr.js"
  },
  "keywords": [
    "svg",
    "png",
    "css",
    "datauri",
    "fallback"
  ],
  "dependencies": {
    "bluebird": "^2.9.25",
    "chalk": "^1.0.0",
    "cssbeautify": "^0.3.0",
    "gzip-size": "^1.0.0",
    "lodash": "^3.0.0",
    "microtime": "^2.1.0",
    "mime": "^1.2.0",
    "mkdirp": "^0.5.0",
    "phantomjs": "^1.9.0",
    "progger": "^1.0.0",
    "rimraf": "^2.2.0",
    "svgo": "^0.5.0",
    "yargs": "^3.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "del": "^2.2.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^2.0.0",
    "gulp-help": "^1.6.1",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-template": "^4.0.0",
    "gulp-util": "^3.0.7",
    "json2markdown": "^1.0.0",
    "mocha": "^2.5.3",
    "run-sequence": "^1.2.2"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/gulp lint",
    "clean": "./node_modules/.bin/gulp clean",
    "build": "./node_modules/.bin/gulp build",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okize/iconr.git"
  },
  "bugs": "https://github.com/okize/iconr/issues",
  "homepage": "https://github.com/okize/iconr/",
  "author": {
    "name": "Morgan Wigmanich",
    "email": "okize123@gmail.com",
    "url": "https://github.com/okize"
  },
  "license": "MIT"
}
