{
  "name": "iconr",
  "version": "0.5.5",
  "description": "CLI tool that parses a directory of SVG images and outputs a CSS file of inlined data URIs as well as a directory of PNG images for fallback.",
  "preferGlobal": true,
  "main": "./bin/iconr.js",
  "bin": {
    "iconr": "./bin/iconr.js"
  },
  "keywords": [
    "svg",
    "png",
    "css",
    "datauri",
    "fallback"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "cssbeautify": "0.3.x",
    "lodash": "^3.0.0",
    "microtime": "^1.2.0",
    "mime": "^1.2.0",
    "mkdirp": "^0.5.0",
    "optimist": "^0.6.0",
    "phantomjs": "^1.9.0",
    "progger": "^0.1.0",
    "q": "^1.0.0",
    "rimraf": "^2.2.0",
    "svgo": "^0.5.0",
    "update-notifier": "^0.3.0"
  },
  "devDependencies": {
    "coffee-script": "*",
    "gulp": "*",
    "gulp-bump": "*",
    "gulp-clean": "*",
    "gulp-coffee": "*",
    "gulp-coffeelint": "*",
    "gulp-help": "*",
    "gulp-template": "*",
    "gulp-util": "*",
    "run-sequence": "*"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okize/iconr.git"
  },
  "bugs": "https://github.com/okize/iconr/issues",
  "homepage": "https://github.com/okize/iconr/",
  "author": {
    "name": "Morgan Wigmanich",
    "email": "okize123@gmail.com",
    "url": "https://github.com/okize"
  },
  "license": "MIT"
}
