{
  "name": "iconr",
  "version": "0.8.5",
  "description": "CLI tool that consumes a directory of SVG images and outputs a CSS file of inlined data as well as a directory of PNG images for fallback if necessary.",
  "preferGlobal": true,
  "engines": {
    "node": "^8.10.0 || ^10.13.0 || >=11.10.1"
  },
  "main": "./bin/iconr.js",
  "bin": {
    "iconr": "./bin/iconr.js"
  },
  "files": [
    "bin",
    "lang",
    "lib"
  ],
  "scripts": {
    "docs": "node ./generate-readme.js",
    "example": "node ./bin/iconr ./example/svg ./example/css --analytics --debug --pretty --verbose --filename=\"icons.css\" --classname=\"icon-\"",
    "example:server": "http-server ./example -p 4321 -o",
    "lint": "npm run test -- --fix --cache && echo \"eslint: no lint errors found\" || true",
    "preversion": "npm run docs",
    "test": "eslint . "
  },
  "keywords": [
    "svg",
    "png",
    "css",
    "datauri",
    "fallback"
  ],
  "dependencies": {
    "bluebird": "^3.5.3",
    "chalk": "^1.0.0",
    "cssbeautify": "^0.3.0",
    "gzip-size": "^3.0.0",
    "mime": "^1.6.0",
    "mkdirp": "^0.5.0",
    "phantomjs-prebuilt": "^2.1.16",
    "progger": "^1.0.0",
    "rimraf": "^2.6.3",
    "svgo": "^1.2.2",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.18.0",
    "http-server": "^0.11.0",
    "json2markdown": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okize/iconr.git"
  },
  "bugs": "https://github.com/okize/iconr/issues",
  "homepage": "https://github.com/okize/iconr/",
  "author": {
    "name": "Morgan Wigmanich",
    "email": "okize123@gmail.com",
    "url": "https://github.com/okize"
  },
  "license": "MIT"
}
