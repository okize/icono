{
  "name": "iconr",
  "version": "0.4.5",
  "description": "CLI tool that takes a directory of SVG images and outputs a CSS file and a directory of PNG images as a fallback.",
  "preferGlobal": true,
  "main": "./bin/iconr.js",
  "bin": {
    "iconr": "./bin/iconr.js"
  },
  "keywords": [
    "svg",
    "png",
    "css",
    "datauri",
    "fallback"
  ],
  "dependencies": {
    "colors": "0.6.x",
    "cssbeautify": "0.3.x",
    "lodash": "2.4.*",
    "microtime": "0.6.x",
    "mime": "1.2.x",
    "mkdirp": "0.5.x",
    "optimist": "0.6.x",
    "phantomjs": "1.9.x",
    "progger": "0.1.x",
    "q": "1.0.x",
    "rimraf": "2.2.x",
    "svgo": "0.4.x"
  },
  "devDependencies": {
    "coffee-script": "*",
    "gulp": "*",
    "gulp-clean": "*",
    "gulp-coffee": "*",
    "gulp-coffeelint": "*",
    "gulp-template": "*",
    "gulp-util": "*"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=1.2.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okize/iconr.git"
  },
  "bugs": "https://github.com/okize/iconr/issues",
  "homepage": "https://github.com/okize/iconr/",
  "author": {
    "name": "Morgan Wigmanich",
    "email": "okize123@gmail.com",
    "url": "https://github.com/okize"
  },
  "license": "MIT"
}
